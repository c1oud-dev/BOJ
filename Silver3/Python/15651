#17:50 - 18:00

import sys
input = sys.stdin.readline

N, M = map(int, input().split())
list = []
for i in range(1, N+1):
    list.append(i)
from itertools import product
for i in product(set(list), repeat = M):
    for j in i:
        print(j, end = ' ')
    print(sep = '\n')
    


